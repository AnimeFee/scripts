PlayerTab:AddToggle({
    Name = "Auto Click",
    Default = false,
    Callback = function(Value)
        G.Click = Value
        if Value then
            while G.Click do
            local args = {
                [1] = "Attack",
                [2] = "Click",
                [3] = {
                ["Type"] = "Defense"
                }
                }
                game:GetService("ReplicatedStorage"):WaitForChild("Bridge"):FireServer(unpack(args))
                wait(.01)
            end
        end
    end
})

