local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()
if not OrionLib then
error("Failed to load the OrionLib library")
return
end

    local Window = OrionLib:MakeWindow({
        Name = "Cool Hub",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "WendelCfg",
        IntroEnabled = false
    })

    -- Table to store global variables
    local G = {
    EGG1 = false,
    EGG2 = false,
    EGG3 = false,
    EGG4 = false,
    EGG5 = false,
    EGG6 = false,
    Upgrade = false,
    Click = false,
    Damage = false,
    Defense = false,
    Luck = false,
    Speed = false,
    Code = false,
    Group = false,
    Daily = false,
    Gifts = false,

}

    local v1 = Window:MakeTab({
        Name = "Scripts",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
 
   local v2 = Window:MakeTab({
        Name = "Eggs",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
    local v3 = Window:MakeTab({
        Name = "Stats",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
 
   local v4 = Window:MakeTab({
        Name = "Teleport",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

v2:AddToggle({
Name = "Auto Buy Egg(10 Wins)",
Default = false,
Callback = function(Value)
G.EGG1 = Value
if Value then
while G.EGG1 do
local args = {
    [1] = 1,
    [2] = "1",
    [3] = {}
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Hatch"):WaitForChild("EggHatch"):InvokeServer(unpack(args))
end
end
end})

