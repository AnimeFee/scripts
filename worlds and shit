# WORLD 1
local args = {
    [1] = "Attack",
    [2] = "Click",
    [3] = {
        ["Enemy"] = workspace:WaitForChild("Server"):WaitForChild("Enemies"):WaitForChild("World"):WaitForChild("Clown Island"):WaitForChild("Bandit"),
        ["Type"] = "World"
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("Bridge"):FireServer(unpack(args))
