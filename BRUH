		if (tostring(v109.ornamentId) == "410020") then
			local v156 = "None";
			if (tostring(v109.ornamentId) == "410020") then
				v156 = "Rainbow";
			end
			print("!! REROLL FOUND !! : " .. v156);
			v72 = true;
		end
	until v72 
	print("Script stopped !");
end
