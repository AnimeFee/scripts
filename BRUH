v22:AddButton({Name="Fps Booster",Callback=function()
	for v111, v112 in pairs(game.ReplicatedStorage.SkillEffects:GetDescendants()) do
		if (v112:IsA("Part") or v112:IsA("MeshPart")) then
			v112.Transparency = 1;
		end
		if (v112:IsA("ParticleEmitter") or v112:IsA("Beam")) then
			v112.Enabled = false;
		end
	end
	game.ReplicatedStorage.Effect.AttackNum.Enabled = false;
	local v74 = game;
	local v75 = v74.Workspace;
	local v76 = v74.Lighting;
	local v77 = v75.Terrain;
	v77.WaterWaveSize = 0;
	v77.WaterWaveSpeed = 0;
	v77.WaterReflectance = 0;
	v77.WaterTransparency = 0;
	v76.GlobalShadows = false;
	v76.FogEnd = 8999999488;
	v76.Brightness = 0;
	settings().Rendering.QualityLevel = "Level01";
	for v113, v114 in pairs(v74:GetDescendants()) do
		if (v114:IsA("Part") or v114:IsA("Union") or v114:IsA("CornerWedgePart") or v114:IsA("TrussPart")) then
			v114.Reflectance = 0;
		elseif (v114:IsA("Decal") or v114:IsA("Texture")) then
			v114.Transparency = 0;
		elseif (v114:IsA("ParticleEmitter") or v114:IsA("Trail")) then
			v114.Lifetime = NumberRange.new(0);
		elseif v114:IsA("Explosion") then
			v114.BlastPressure = 0;
			v114.BlastRadius = 0;
		elseif (v114:IsA("Fire") or v114:IsA("SpotLight") or v114:IsA("Smoke") or v114:IsA("Sparkles")) then
			v114.Enabled = false;
		elseif v114:IsA("MeshPart") then
			v114.Reflectance = 0;
		end
	end
	for v115, v116 in pairs(v76:GetChildren()) do
		if (v116:IsA("BlurEffect") or v116:IsA("SunRaysEffect") or v116:IsA("ColorCorrectionEffect") or v116:IsA("BloomEffect") or v116:IsA("DepthOfFieldEffect")) then
			v116.Enabled = false;
		end
	end
	sethiddenproperty(game.Lighting, "Technology", "Compatibility");
end});
