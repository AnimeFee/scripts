function rerollLegendaryMask()
	local v65 = false;
	repeat
		local v94 = {[1]=400001};
		local v95 = game:GetService("ReplicatedStorage").Remotes.RerollOrnament:InvokeServer(unpack(v94));
		wait(0.1);
		if ((tostring(v95.ornamentId) ~= "410001") and (tostring(v95.ornamentId) ~= "410002") and (tostring(v95.ornamentId) ~= "410003") and (tostring(v95.ornamentId) ~= "410004") and (tostring(v95.ornamentId) ~= "410005")) then
			print("Unknown reroll found ! : " .. tostring(v95.ornamentId));
			v65 = true;
		end
		if ((tostring(v95.ornamentId) == "410004") or (tostring(v95.ornamentId) == "410005")) then
			local v149 = "None";
			if (tostring(v95.ornamentId) == "410004") then
				v149 = "Legendary";
			elseif (tostring(v95.ornamentId) == "410005") then
				v149 = "Rainbow";
			end
			print("!! REROLL FOUND !! : " .. v149);
			v65 = true;
		end
	until v65 
	print("Script stopped !");
end
